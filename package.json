{
  "name": "my-custom-store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build-component": "yarn esbuild src/components/*.tsx --bundle --minify --outdir=public --jsx=automatic --format=esm --platform=browser --external:react --external:react-dom",
    "prepare": "husky"
  },
  "dependencies": {
    "@makerinc/nav-sdk": "^0.0.6",
    "esbuild": "^0.25.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.14",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "packageManager": "yarn@4.6.0"
}
